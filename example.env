# Your Discord bot token; keep this safe!
DISCORD_TOKEN=
# A unique key to use for the bot websocket endpoint; this should be sufficiently random enough that
# brute forcing this would be largely impossible.
BOT_KEY=
# The channel ID that the bot should bridge messages from/to
BRIDGE_CHANNEL=
# The port that the bridge server runs on; note that this is not used by the actual server!
# The port must instead be set when you run the server, e.g. with uvicorn '--port 8000'
BRIDGE_PORT=8000
